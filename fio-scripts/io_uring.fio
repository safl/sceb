[global]
thread=1
bs=512
direct=1
iodepth=128
cpus_allowed=0,1
cpus_allowed_policy=split
rw=randread
allow_file_create=0
runtime=10
ramp_time=2
group_reporting=1

ioengine=io_uring
hipri=1
fixedbufs=1
registerfiles=1

[dev1]
filename=/dev/nvme0n1

[dev2]
filename=/dev/nvme1n1

[dev3]
filename=/dev/nvme2n1

[dev4]
filename=/dev/nvme3n1

[dev5]
filename=/dev/nvme4n1

[dev6]
filename=/dev/nvme5n1

[dev7]
filename=/dev/nvme6n1

[dev8]
filename=/dev/nvme7n1
